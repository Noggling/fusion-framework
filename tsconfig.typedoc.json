{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "removeComments": false,
    "declaration": true,
  },
  "typedocOptions": {
    "entryPoints": [
      "./packages/framework",
      "./packages/framework-react",
      "./packages/module",
      "./packages/module-http",
      "./packages/module-msal",
      "./packages/module-service-discovery",
      "./packages/react-app",
    ],
    "entryPointStrategy": "packages",
    "readme": "./README.md",
    "out": "docs",
    "hideGenerator": true,
    "excludeExternals": true,
    "includes": [
      "./packages/framework-react"
    ],
  }
}